{
  "name": "lynx-devtool",
  "productName": "Lynx DevTool",
  "version": "0.0.1-beta.2",
  "description": "Lynx Devtool",
  "main": "dist/index.js",
  "scripts": {
    "build": "rsbuild build && cp preload.js dist/",
    "start": "rsbuild build && electron .",
    "dev:build": "rsbuild build",
    "dev:serve": "rsbuild dev",
    "dev": "pnpm run build:all && concurrently \"pnpm run dev:serve\" \"wait-on dist/index.js && electron .\"",
    "build:all": "pnpm --filter @lynx-dev/* run build && pnpm run build && mkdir -p dist/lynx-devtool-web && cp -r packages/lynx-devtool-web/dist/* dist/lynx-devtool-web/",
    "clean": "rm -rf dist && pnpm -r exec rm -rf dist && rm -rf packages/devtools-frontend-lynx/out packages/devtools-frontend-lynx/output",
    "rebuild": "pnpm run clean && pnpm run build",
    "build:devtools-frontend-lynx": "pnpm run fetch:depot_tools && export PATH=$(pwd)/packages/devtools-frontend-lynx/buildtools/depot_tools:$PATH && pnpm run sync:devtools-gn && pnpm run build:devtools && pnpm run sync:devtools-dist",
    "fetch:depot_tools": "cd packages/devtools-frontend-lynx && scripts/fetch-depot-tools.sh",
    "sync:devtools-gn": "gclient sync --gclientfile=.gclient --deps=all",
    "build:devtools": "cd packages/devtools-frontend-lynx && scripts/build-lynx-devtools.sh",
    "sync:devtools-dist": "scripts/sync-devtools-output.sh",
    "production:mac-arm64": "pnpm run clean && pnpm run build:all && cross-env NODE_ENV=production electron-builder --arm64",
    "production:mac-x64": "pnpm run clean && pnpm run build:all && cross-env NODE_ENV=production electron-builder --x64"
  },
  "author": "lynx",
  "license": "Apache-2.0",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@lynx-dev/lynx-devtool-cli": "workspace:*",
    "@lynx-dev/lynx-devtool-utils": "workspace:*",
    "@lynx-js/remote-debug-driver": "0.0.2",
    "@monaco-editor/react": "4.5.0",
    "@paciolan/remote-module-loader": "3.0.3",
    "axios": "^1.8.1",
    "detect-port": "^1.6.1",
    "electron-log": "^4.4.8",
    "electron-progressbar": "2.2.1",
    "electron-prompt": "^1.7.0",
    "glob": "^11.0.1",
    "monaco-editor": "0.37.1",
    "multer": "1.4.5-lts.1",
    "node-machine-id": "1.1.12",
    "normalize.css": "^8.0.1",
    "qrcode.react": "^4.2.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-hot-loader": "^4.13.1",
    "sharp": "^0.32.6",
    "source-map-support": "^0.5.21",
    "ts-loader": "^6.2.2",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@rsbuild/core": "^1.2.12",
    "@rsbuild/plugin-babel": "^1.0.4",
    "@rsbuild/plugin-type-check": "^1.2.1",
    "@types/electron-prompt": "1.6.5",
    "@types/node": "^18.19.76",
    "@types/react": "^16.14.62",
    "@types/source-map-support": "^0.5.10",
    "babel-loader": "^8.4.1",
    "concurrently": "^5.3.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.6.0",
    "defer-to-connect": "~2.0.1",
    "del-cli": "^3.0.1",
    "electron": "22.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "8.20.0",
    "sass": "1.58.0",
    "sass-loader": "^13.3.3",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "wait-on": "7.0.1"
  },
  "build": {
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      {
        "from": "src/res/icons",
        "to": "icons",
        "filter": [
          "**/*"
        ]
      }
    ],
    "appId": "com.lynx.devtool",
    "productName": "LynxDevTool",
    "extraMetadata": {
      "env": "production"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ],
      "artifactName": "Lynx-DevTool-${version}-${arch}.dmg",
      "icon": "res/icons/lynx-devtool.icns"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": null
  },
  "pnpm": {
    "overrides": {
      "electron": "22.0.1",
      "@types/electron": "22.0.1"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "7.33.6"
  },
  "packageManager": "pnpm@7.33.6"
}
